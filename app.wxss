@import 'we7/resource/style/we7.wxss';
@import 'we7/resource/style/weui.wxss';
@import 'super_card/components/wux.wxss';
@import 'super_card/resource/style/iconfont4.wxss';
@import 'super_card/resource/style/iconfont3.wxss';
@import 'super_card/resource/style/iconfont2.wxss';
@import 'super_card/resource/style/iconfont.wxss';

.img-fit{
  position: relative;
}
page{
  background-color: #f5f5f5;
}
.rz-v{
  color:#fff;
  background-color:#f90;
  font-size:26rpx !important;
  border-radius:50%;
  width:34rpx;
  height:34rpx;
  text-align:center;
  margin:0 5rpx;
  font-family:"FZDaBiaoSong-B06S";
  line-height:36rpx;
  position:absolute;right:2px;bottom:4px;
}


.icon-creation-prompts{
  height:80rpx;
  width:80rpx;
  position: fixed;
  top:190rpx;
  right: 10px;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  display: -webkit-flex;
  -webkit-flex-flow: column;
  -webkit-justify-content: center;
  -webkit-align-items: center;
  z-index:999;
  border-radius:50%;
  background: rgba(255,200,22, .6);
  text-align: center;
}
.creation-prompts-img{
  height:65rpx;
  width:65rpx;
  font-size:38rpx;
  line-height:65rpx;
  border-radius:50%;
  background-color:#ffc816;
  color:#fff;
  text-align:center;
  font-weight: 800;
}
.creation-prompts-text{
  color:#ffc816;
  font-size: 10px;
}

.goTop{
  width:80rpx;
  height:78rpx;
  border-radius:80rpx;
  text-align:center;
  line-height:80rpx;
  position:fixed;
  right:10px;
  bottom:12%;
  font-size:80rpx;
  color:#bbb;
  background:#fff;
  z-index:999;
  }

  .L-T-icon::after{
    border: 0;
  }
  .L-T-icon{
    position:fixed;
    right:10px;
    bottom:12%;
    width:88rpx;
    height:88rpx;
    line-height: inherit;
    border-radius:50%;
    background:#fff;
    z-index:999;
    filter:drop-shadow(0px 0px 5px #ccc);
    display: flex;
    align-items: center;
    justify-content: center;
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: center;
  }
  .L-T-icon text{
    text-align:center;
    line-height:50rpx;
    font-size:50rpx;
    color:#00a1e9;
  }
  .L-T-sum{
    width: 40rpx;
    height: 40rpx;
    font-size: 24rpx;
    background-color: red;
    border-radius: 50%;
    color: #fff;
    position:absolute;
    top:-5rpx;
    right:-5rpx;
    display: flex;
    align-items: center;
    justify-content: center; 
    display: -webkit-flex;
    -webkit-align-items: center;
    -webkit-justify-content: center;    
    overflow: hidden;

  }

.menu-form{
  /* flex: 1;
  -webkit-flex: 1; */
  width: 25%;
}

.L-T-icon{
  overflow:visible;
  padding: 0;
}


.ove-btn{
  width:88rpx;
  height:88rpx;
  border-radius:50%;
  display:flex;
  align-items:center;
  justify-content:center;
  display:-webkit-flex;
  -webkit-align-items:center;
  -webkit-justify-content:center;
  padding:0;
  margin:0;
  position:fixed;
  right:10px;
  bottom:21%;
  z-index:999;
  filter:drop-shadow(0px 0px 5px #ccc);
  background-color: #fff;
}
.ove-btn .iconfont{
  font-size:50rpx;
  line-height:50rpx;
  color:#00a1e9;
}
.ove-btn::after{
  border: 0;
}
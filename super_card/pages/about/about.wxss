.contains{
  font-size: 36rpx;
}
.content,.present{
  text-align: center;
  margin-top:70rpx;
}
.content image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 20rpx;
}
.content text,.present text{
  display: block;
  text-align: center;
  color: #5b5b5b;
  font-size: 40rpx;
}
.present{
  margin:30rpx 0;
}
.present text{
  /* color: #000;
  font-size: 30rpx; */
  font-size:28rpx;
  color:#9e9e9e;
  text-align: left;
  padding: 0 30rpx;
}
.present image{
  width: 300rpx;
  height: 100rpx;
}
.bottom{
  font-size: 26rpx;
  color: #9e9e9e;
  position:fixed;
  width:100%;
  bottom:20rpx;
}
.bottom text{
  display: block;
  text-align: center;
}

.company-introduce{
  margin-top: 15rpx;
  font-size: 22rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
}
.superior-products{
  margin-top: 30rpx;
  position: relative;
  padding-right: 35rpx;
}
.arrow-right{
  position: absolute;
  right: 0;
  font-size: 28rpx;
  color: #f5f5f5;
  top: 22rpx;
  opacity: 0.5;
}
.scroll-view{
  height: 100rpx;
  width:100%;
  white-space:nowrap;
}
.product-item{
  display:inline-block;
  width:100rpx;
  margin-left:20rpx;
  overflow:hidden;
}
.product-item:first-child{
  margin-left: 0;
}
.product-itemImg{
  height:100rpx;
  width:100rpx;
  display:block;
  overflow:hidden;
  text-align: center;
  background-size: cover;
  border-radius:13rpx;
  box-shadow: 0 8rpx 10rpx rgba(0,0,0,.2);
  background: #fff;
}
.superior-homePage,.superior-phone{
  width: 50rpx;
  height: 50rpx;
  border: 3rpx solid #fff;
  position: absolute;
  border-radius: 50%;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 50rpx;
}
.superior-homePage{
  left: 15rpx;
  top: 10rpx;
}
.superior-phone{
  left: 15rpx;
  bottom: 10rpx;
}
.superior-headImg{
  width: 118rpx;
  height: 150rpx;
  position: absolute;
  right: 15rpx;
  top: 20%;
  font-size: 22rpx;
  color: #f6d96c;
  text-align: center;
}
.superior-headImg-img{
  width: 118rpx;
  height: 118rpx;
  border-radius: 50%;
  background: #fff;

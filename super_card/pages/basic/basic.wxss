/* 上级信息 */
.superior-info{
  width: 100%;
  padding: 15rpx 10rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  position: relative;
}
.superior-industry{
  height: 35rpx;
  display:flex;
  display:-webkit-flex;
  justify-content: flex-end;
}
.superior-industry-item{
  margin-left:15rpx;
  padding:0 15rpx;
  height:35rpx;
  text-align:center;

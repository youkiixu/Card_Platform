}
.ther-ll{
  padding: 30rpx;
  background-color: #e5e5f7;
  border-radius: 50%;
  animation:wow linear .8s infinite;
}

@keyframes wow {
  0%{
    padding: 0rpx;
  }
  25%{
    padding: 15rpx;
  }
  50%{
    padding: 30rpx;
  }
  75%{
    padding: 15rpx;
  }
  100%{
    padding: 0rpx;
  }
}


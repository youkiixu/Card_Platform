.popup-hide {
  z-index: 9999;
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.6);
}

.popup {
  width: 100%;
  height: 100%;
  z-index: 99999;
  position: fixed;
  top: 0;
  background-color: rgba(0, 0, 0, 0.6);
  display: flex;
  align-items: flex-end;
  display: -webkit-flex;
  -webkit-align-items: flex-end;
}

.popup-content {
  background-color: #fff;
  width: 100%;
  display: flex;
  flex-flow: column;
  display: -webkit-flex;
  -webkit-flex-flow: column;
  position: fixed;
  bottom: 0;
  z-index: 99999;
}

.popup-act {
  display: flex;
  flex-wrap: wrap;
  display: -webkit-flex;
  -webkit-flex-wrap: wrap;
  font-size: 34rpx;
}

.popup-act view {
  border-radius: 10rpx;
  border: 1px solid #bebebe;
  padding: 14rpx 30rpx;
  margin: 20rpx 20rpx;
}

.popup-del {
  font-size: 34rpx;
  border-top: 10rpx solid #f5f5f5;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
}

.wx-switch-input{width:42px !important;height:22px !important;}
.wx-switch-input::before{width:41px !important;height: 22px !important;}
.wx-switch-input::after{width: 20px !important;height: 20px !important;}
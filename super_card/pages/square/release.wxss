/* super_card/pages/release/release.wxss */
.contains{
  font-size: 32rpx;
  font-family: '微软雅黑';
}
.trans{
  transform:rotate(270deg);
  -webkit-transform:rotate(270deg);
  color:#c8c8c8;
}

.top{
  margin-top: 20rpx;
}
.top-type{
  height: 90rpx;
  background-color: #fff;
  padding: 0 3vw;
  display: flex;
  align-items: center;
  justify-content: space-between;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: space-between;
}
.type-l,.type-r{
  flex: 1;
  display: flex;
  align-items: center;
  -webkit-flex: 1;
  display: -webkit-flex;
  -webkit-align-items: center;
}
.type-l .iconfont{
  font-size: 14rpx;
  line-height: 14rpx;
  margin-left: 16rpx;
  color: red;
}
.type-r input{
  text-align: right;
}
.top-tag{
  padding: 0 3vw;
  background-color: #fff;
  margin-top: 1px;
  display: flex;
  flex-flow: column;
  align-items: flex-start;
  display: -webkit-flex;
  -webkit-flex-flow: column;
  -webkit-align-items: flex-start;
}
.top-tag .type-l{
  line-height: 80rpx;
}
.tag-r{
  padding-bottom: 20rpx;
}
.free-radios{
  display:flex;
  flex-wrap:wrap;
  display:-webkit-flex;
  -webkit-flex-wrap:wrap;
  margin-left:-4rpx;
}
.free-radio{
  padding:4rpx 30rpx;
  border: 1px solid #c8c8c8;
  border-radius: 4rpx;
  margin: 8rpx;
}
.is_checked{
  border-color: #00a1e9;
  color:#00a1e9;
}
.free-text{
  font-size: 30rpx;
}
.content{
  margin-top: 20rpx;
  background-color: #fff;
  padding: 50rpx 2vw;
}
.con-t{
  width: 100%;
  font-size: 30rpx;
  overflow-y:hidden;
  padding:0 1vw;
}
.content-imgs{
  margin-top: 70rpx;
  display: flex;
  flex-wrap: wrap;
  display: -webkit-flex;
  -webkit-flex-wrap: wrap;
}
.content-img{
  width: 30vw;
  height: 30vw;
  margin: 1vw;
  padding-top: 1px;
  position:relative;
}
.delete{
  position:absolute;
  color:#fff;
  top:-6px;
  right:-7px;
  font-size:10px;
  line-height:10px;
  width:14px;
  height:14px;
  background-color:rgba(0, 0, 0, 0.5);
  border-radius:50%;
  display: flex;
  align-items: center;
  justify-content: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  transform:rotate(45deg);
  -webkit-transform:rotate(45deg);
}
.content-plus{
  width: 30vw;
  height: 30vw;
  margin: 1vw;
}
.content-img .cp-icon,.content-plus .iconfont{
  height: inherit;
  font-size: 160rpx;
  line-height: 160rpx;
  color: #cbcbcb;
  display: flex;
  align-items: center;
  justify-content: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  border: 1px solid #cbcbcb;
}
.content-img image{
  width: inherit;
  height: inherit;
}

.protocol{
  margin-top: 20rpx;
  background-color: #fff;
  line-height: 90rpx;
  padding: 0 3vw;
  font-size: 30rpx;
}
.protocol checkbox .wx-checkbox-input{
   width: 30rpx; /* 背景的宽 */
   height: 30rpx; /* 背景的高 */
}
.protocol-word{
  font-size:30rpx;
  margin-left:10rpx;
}
.protocol-word text{
  color:#00a1e9;
  text-decoration: underline;
  margin-left: 2rpx;
}

.btn-submit{
  padding:60rpx 30rpx;
}
.btn-block{
  line-height: 90rpx;
  border-radius:50rpx;
}
.btn-blue{
  background: #00a1e9;
  color: #fff;
}

.info-choice{
  margin-top: 20rpx;
  background-color: #fff;
  padding:30rpx;
  display:flex;
  align-items:center;
  justify-content:space-between;
  display:-webkit-flex;
  -webkit-align-items:center;
  -webkit-justify-content:space-between;
}
.info-img{
  width:150rpx;
  height:150rpx;
  border-radius:5px;
}
.info-card{
  flex:1;
  display:flex;
  flex-flow:column;
  align-items:flex-start;
  -webkit-flex:1;
  display:-webkit-flex;
  -webkit-flex-flow:column;
  -webkit-align-items:flex-start;
  height:150rpx;
  margin-left:20rpx;
  overflow:hidden;
}
.card-name{
  color:#000;
  height:50rpx;
  line-height:50rpx;
}
.card-phone,.card-company{
  color:#9f9f9f;
  font-size:28rpx;
  height:50rpx;
  line-height:50rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.choice-ah{
  flex:1;
  display:flex;
  flex-flow:column;
  align-items: center;
  -webkit-flex:1;
  display:-webkit-flex;
  -webkit-flex-flow:column;
  -webkit-align-items: center;
}
.choice-ah .iconfont{
  width:100rpx;
  height:100rpx;
  font-size:70rpx;
  line-height:70rpx;
  background-color:#00a1e9;
  border-radius:50%;
  color:#fff;
  display: flex;
  align-items: center;
  justify-content: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  filter:drop-shadow(0px 0px 2px #00a1e9);
}
.choice-ahead{
  font-size: 26rpx;
  color: #cbcbcb;
  line-height: 50rpx;
}





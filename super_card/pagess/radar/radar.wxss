/* 删除聊天记录 */
movable-area{
  width: 100vw;
  height: 19vw;
  background-color:#fff;
}
movable-view{
  width: 100vw;
  height: 19vw;
  z-index:300;
  background-color:#fff;
}
.main-items{
  overflow: hidden;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #e2e2e2;
}
.main-items:last-child{
  border: 0;
}
.delete-btn{
  width:200rpx;
  font-size:28rpx;
  letter-spacing:4rpx;
  background-color:#e8423f;
  color:#fff;
  display:flex;
  align-items:center;
  justify-content:center;
  position:relative;
  z-index:100;
}
/* super_card/pages/radar/radar.wxss */

.ck{
  color: #00a1e9;
}

/*去掉滚动条*/
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}

.radar-top{
  display: flex;
  align-items: center;
  background-color: #fff;
  border-bottom:1px solid #e5e5e5;
  border-top:1px solid #e5e5e5;
  padding:0 2vw;
  position: relative;
}
.radar-title{
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  text-align: center;
  border-bottom: 3px solid #fff;
  position: relative;
}
.lt-infonum{
  width:36rpx;
  height:36rpx;
  line-height:36rpx;
  text-align:center;
  border-radius:50%;
  background-color:red;
  color:#fff;
  font-size:10px;
  position:absolute;
  top:20rpx;
  right:50rpx;
  overflow: hidden;    
  text-overflow:ellipsis;    
  white-space: nowrap;
}
/* .radar-top .on{
  color:#00a1e9;
  border-color: #00a1e9;
} */

.visitor{
  margin-top: 1px;
}
.visitor-top{
  display: flex;
  align-items: center;
}
.visitor-top .visitor-top-btn{
  flex: 1;
  font-size: 28rpx;
  text-align: center;
  line-height: 90rpx;
  color: #999999;
}
.visitor-top .visitor-top-btn text{
  border-radius: 6rpx;
  padding: 8rpx 30rpx;
  letter-spacing: 4rpx;
}
.visitor-top .coin text{
  background-color: #00a1e9;
  color: #fff;
}

.trail-act{
  display: flex;
  align-items: center;
  padding: 20rpx;
  margin:0 30rpx 30rpx 30rpx;
  background-color: #fff;
  border: 1px solid #eaeaea;
  border-radius: 6rpx;
}
.trail-act-img{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 1px solid #999999;
  margin-right: 20rpx;
}
.trail-act-info{
  flex: 1;
}
.trail-act-info-top{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.trail-act-name{
  font-size: 32rpx;
}
.trail-act-time{
  font-size: 26rpx;
  color: #bfbfbf;
}
.trail-act-news{
  margin-top: 10rpx;
  color: #a9a9a9;
  font-size: 26rpx;
}
.visitor-act{
  background-color: #fff;
  padding:20rpx;
  display: flex;
  align-items: center;
  border-bottom:1px solid #e5e5e5;
}
.visitor-act-img{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 1px solid #999999;
  margin-right: 20rpx;
}
.visitor-act-info{
  flex: 1;
}
.visitor-act-info-ck{
  margin: 0 10rpx;
  letter-spacing: 4rpx;
  font-size: 26rpx;
  color: #bfbfbf;
}
.trans{
  font-size: 26rpx;
  color: #bfbfbf;
  transform:rotate(180deg);
  -webkit-transform:rotate(180deg);
}
.visitor-act-info-top{
  font-size: 32rpx;
}
.visitor-act-info-content{
  margin-top: 10rpx;
  font-size: 26rpx;
  color: #a9a9a9;
}

.analysis-act{
  display: flex;
  align-items: center;
  padding: 20rpx;
  background-color: #fff;
  border-bottom: 1px solid #e5e5e5;
}
.analysis-act-img{
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
  border-radius: 6rpx;
  /*border:1px solid #999999;*/
}
.analysis-act-info{
  flex:1;
  display: flex;
  flex-flow: column;
}
.analysis-act-name{
  font-size: 34rpx;
}
.analysis-act-bq{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.analysis-act-bq view{
  font-size:24rpx;
  padding:4rpx 10rpx;
  letter-spacing:2rpx;
  background-color:#e1f5de;
  color:#00a1e9;
  margin:4rpx 8rpx;
  border-radius:6rpx;
}
.analysis-act-bq view:first-child{
  margin-left: 0;
}
.analysis-act-time{
  font-size: 26rpx;
  color: #bfbfbf;
}
.analysis-act-sum{
  font-size: 28rpx;
  color: #999999;
  text-align:center;
  margin:30rpx 0;
}


.cose-chat{
  border-bottom: 1px solid #e5e5e5;
  margin-bottom: 100rpx;
  background-color:#fff;
}
.cose-act{
  padding: 2vw 2vw 2vw 0;
  display: flex;
  align-items: center;
  justify-content:flex-start;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content:flex-start;
  /* overflow: hidden; */
}

.act-left{
  flex:1;
  display: flex;
  align-items: center;
  -webkit-flex:1;
  display: -webkit-flex;
  -webkit-align-items: center;
}
.act-left-info{
  border: 1px solid #ceced0;
  border-radius: 50%;
  display: flex;
  display: -webkit-flex;
  margin-right: 2vw;
  margin-left: 2vw;
  position: relative;
}
.act-left-tr{
  width: 40rpx;
  height: 40rpx;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;
  color: #fff;
  background-color: red;
  position: absolute;
  top: -8rpx;
  right: -8rpx;
  border-radius: 50%;
  
}
.act-left-img{
  width: 15vw;
  height: 15vw;
  border-radius: 50%;
}
.act-centre{
  flex: 1;
  display: flex;
  flex-flow: column;
  align-items: flex-start;
  -webkit-flex: 1;
  display: -webkit-flex;
  -webkit-flex-flow: column;
  -webkit-align-items: flex-start;
}
.act-centre-say{
  width:77vw;
  color: #ceced0;
  font-size: 30rpx;
  overflow: hidden;    
  text-overflow:ellipsis;    
  white-space: nowrap;
}
.act-right{
  margin-left: 30rpx;
  font-size: 26rpx;
  color: #ceced0;
  white-space:nowrap;
}

.act-centre-top{
  display:flex;
  align-items:center;
  justify-content:space-between;
  display:-webkit-flex;
  -webkit-align-items:center;
  -webkit-justify-content:space-between;
  width:100%;
}

.vt-ck-coin-d{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100vh;
  background-color:rgba(0,0,0,0.6);
}

.vt-ck-coin{
  position: fixed;
  bottom: 0;
  background:#fff;
  width:100%;
  height:90vh;
}

.vt-ck-coin-top{
  padding:30rpx;
  display:flex;
  align-items:center;
}
.transvt{
  font-size: 26rpx;
  color: #bfbfbf;
}
.vtck-act{
  margin: 0 30rpx 30rpx 30rpx;
  border-radius: 6rpx;
  border: 1px solid #eaeaea;
  box-shadow: 0px 2px 1px rgba(221, 221, 221,0.23);
  padding: 30rpx;
  display: flex;
  flex-flow: column;
}
.vtck-act:last-child{
  margin-bottom: 50rpx;
}
.vtck-act-top{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.vtck-act-name{
  font-size: 32rpx;
}
.vtck-act-time{
  font-size:26rpx;
  color:#a9a9a9;
}
.vtck-act-content{
  margin-top: 10rpx;
  font-size: 28rpx;
  color:#a9a9a9;
}
button.s-not-coin::after{
  border: 0;
}
.s-not-coin{
  width:90rpx;
  height:90rpx;
  line-height: 30rpx;
  border:1px solid #f90;
  border-radius:50%;
  font-size:28rpx;
  display:flex;
  flex-flow:column;
  align-items:center;
  justify-content:center;
  background-color:#fff;
  color:#f90;
  position:fixed;
  bottom:100rpx;
  right:50rpx;
  z-index: 500;
  padding: 0;
  margin: 0;
  overflow: visible;
}
.s-not-sum{
  width:50rpx;
  height:36rpx;
  line-height:36rpx;
  font-size:10px;
  border-radius:50%;
  background-color:red;
  color:#fff;
  text-align:center;
  position:absolute;
  top:-10rpx;
  right: -10rpx;
  overflow: hidden;    
  text-overflow:ellipsis;    
  white-space: nowrap;

}

.vt-content{
  position: relative;
  margin-bottom: 60rpx;
}
.vt-content-top{
  position:absolute;
  background-color:rgba(0,0,0,.5);
  top:0;
  left:0;
  width:100%;
  height:100%;
  z-index:11;
}
.no-pri{
  filter:blur(3px);
}

